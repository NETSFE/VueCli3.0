{
  "name": "cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "stg": "vue-cli-service build --mode stg"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cssnano": "^4.1.0",
    "es6-promise": "^4.2.5",
    "exif-js": "^2.3.0",
    "fetch-jsonp": "^1.1.3",
    "html2canvas": "^1.0.0-alpha.12",
    "jquery": "^3.3.1",
    "js-md5": "^0.7.3",
    "less": "^3.7.0",
    "less-loader": "^4.1.0",
    "lib-flexible": "^0.3.2",
    "postcss-aspect-ratio-mini": "0.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-viewport-units": "^0.1.4",
    "postcss-write-svg": "^3.0.1",
    "px2rem-loader": "^0.1.9",
    "register-service-worker": "^1.0.0",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-touch": "^1.1.0",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.0-beta.15",
    "cssnano-preset-advanced": "^4.0.0",
    "exports-loader": "^0.7.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-px2rem": "^0.3.0",
    "postcss-url": "^8.0.0",
    "script-loader": "^0.7.2",
    "vconsole-webpack-plugin": "^1.4.2",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-url": {},
      "postcss-aspect-ratio-mini": {},
      "postcss-write-svg": {
        "utf8": false
      },
      "postcss-cssnext": {},
      "postcss-px-to-viewport": {
        "viewportWidth": 375,
        "unitPrecision": 3,
        "viewportUnit": "vw",
        "selectorBlackList": [
          ".ignore",
          ".hairlines"
        ],
        "minPixelValue": 1,
        "mediaQuery": false
      },
      "postcss-viewport-units": {},
      "cssnano": {
        "preset": "advanced",
        "autoprefixer": false,
        "postcss-zindex": false
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
